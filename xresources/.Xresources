Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

URxvt.font: xft:Ubuntu Mono:size=13
URxvt.scrollBar: false

URxvt.internalBorder:  16

! special
URxvt.foreground : #c8c8d0
