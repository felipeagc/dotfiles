Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: grayscale
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

rofi.color-enabled: true

!			bg       border   separator
rofi.color-window:	#181818, #263b4c, #181818

! State			bg       fg       bgalt    hlbg     hlfg	
rofi.color-normal:	#181818, #585858, #181818, #181818, #d8d8d8
rofi.color-active:	#181818, #d8d8d8, #181818, #181818, #d8d8d8
rofi.color-urgent:	#181818, #d8d8d8, #181818, #181818, #d8d8d8
rofi.font:		Roboto 15
rofi.bw: 		3
rofi.padding:		32
rofi.width:		50
rofi.lines:		10
rofi.modi:		window,run,ssh,drun

URxvt*font: xft:SF Mono:Semibold:size=11,xft:Iosevka:size=14,xft:Symbola
URxvt*scrollBar: false
URxvt*transparent: false
URxvt*internalBorder: 16

URxvt*keysym.Shift-Control-V: eval:paste_clipboard
URxvt*keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt*iso14755: False

URxvt*perl-ext-common: default,resize-font

URxvt*letterSpace: -1

xterm*utf8: 1

! special
*.foreground:   #dfdfdf
*.background:   #121212
*.cursorColor:  #e4e4e4

! black
*.color0:       #121212
*.color8:       #6c6c6c

! red
*.color1:       #af5f5f
*.color9:       #af5f5f

! green
*.color2:       #d75f5f
*.color10:      #d75f5f

! yellow
*.color3:       #afd7d7
*.color11:      #afd7d7

! blue
*.color4:       #af8787
*.color12:      #af8787

! magenta
*.color5:       #dfaf87
*.color13:      #dfaf87

! cyan
*.color6:       #ffafaf
*.color14:      #ffb2af

! white
*.color7:       #dfdfdf
*.color15:      #dfdfdf
