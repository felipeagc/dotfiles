Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: grayscale
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

rofi.color-enabled: true

!										bg       border   separator
rofi.color-window:	#1d2021, #ebdbb2, #1d2021

! State							bg       fg       bgalt    hlbg     hlfg
rofi.color-normal:	#1d2021, #928374, #1d2021, #1d2021, #ebdbb2
rofi.color-active:	#1d2021, #ebdbb2, #1d2021, #1d2021, #ebdbb2
rofi.color-urgent:	#1d2021, #ebdbb2, #1d2021, #1d2021, #ebdbb2
rofi.font:		      sans 14
rofi.bw: 		        2
rofi.padding:		    32
rofi.width:		      50
rofi.lines:		      10
rofi.modi:		      window,run,ssh,drun

st.termname: st-256color
st.font: DejaVu Sans Mono:pixelsize=12
st.cwscale: 1.0
st.borderpx: 12
st.mouseScrollLines: 5
st.bold_font: 1
st.actionfps: 60
st.xfps: 120

URxvt*font: xft:DejaVu Sans Mono:bold:size=10,xft:Symbola
URxvt*scrollBar: false
URxvt*transparent: false
URxvt*internalBorder: 12

URxvt*keysym.Shift-Control-V: eval:paste_clipboard
URxvt*keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt*iso14755: False

URxvt*perl-ext-common: default,resize-font

URxvt*letterSpace: -1

xterm*utf8: 1


