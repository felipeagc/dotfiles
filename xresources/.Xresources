! green
URxvt*color2  : #98c379
URxvt*color10 : #7a9f60

! yellow
URxvt*color3  : #e5c07b
URxvt*color11 : #d19a66

! blue
URxvt*color4  : #61afef
URxvt*color12 : #3b84c0

! magenta
URxvt*color5  : #c678dd
URxvt*color13 : #9a52af

! cyan
URxvt*color6  : #56b6c2
