Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: grayscale
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

rofi.color-enabled: true

!			bg       border   separator
rofi.color-window:	#181818, #263b4c, #181818

! State			bg       fg       bgalt    hlbg     hlfg	
rofi.color-normal:	#181818, #585858, #181818, #181818, #d8d8d8
rofi.color-active:	#181818, #d8d8d8, #181818, #181818, #d8d8d8
rofi.color-urgent:	#181818, #d8d8d8, #181818, #181818, #d8d8d8
rofi.font:		Roboto 15
rofi.bw: 		3
rofi.padding:		32
rofi.width:		50
rofi.lines:		10
rofi.modi:		window,run,ssh,drun

st.termname: st-256color
st.font: DejaVu Sans Mono:bold:size=10
st.cwscale: 1.0
st.borderpx: 12
st.mouseScrollLines: 5
st.bold_font: 1
st.actionfps: 60
st.xfps: 120

URxvt*font: xft:DejaVu Sans Mono:bold:size=10,xft:Symbola
URxvt*scrollBar: false
URxvt*transparent: false
URxvt*internalBorder: 12

URxvt*keysym.Shift-Control-V: eval:paste_clipboard
URxvt*keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt*iso14755: False

URxvt*perl-ext-common: default,resize-font

URxvt*letterSpace: -1

xterm*utf8: 1


! hard contrast: *background: #1d2021
*background: #282828
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
