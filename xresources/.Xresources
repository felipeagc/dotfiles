Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: grayscale
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

rofi.color-enabled: true

!			bg       border   separator
rofi.color-window:	#181818, #282828, #181818

! State			bg       fg       bgalt    hlbg     hlfg	
rofi.color-normal:	#181818, #585858, #181818, #181818, #d8d8d8
rofi.color-active:	#181818, #d8d8d8, #181818, #181818, #d8d8d8
rofi.color-urgent:	#181818, #d8d8d8, #181818, #181818, #d8d8d8
rofi.font:		Roboto 15
rofi.bw: 		1
rofi.padding:		32
rofi.width:		50
rofi.lines:		10

st.font: Iosevka:size=11:antialias=true:autohint=true;
st.borderpx: 16

URxvt*font: xft:SF Mono:size=12,xft:Iosevka:size=14,xft:Symbola
URxvt*scrollBar: false
URxvt*transparent: false
URxvt*internalBorder: 16

URxvt*keysym.Shift-Control-V: eval:paste_clipboard
URxvt*keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt*iso14755: False

URxvt*perl-ext-common: default,resize-font

URxvt*letterSpace: -1

xterm*utf8: 1

#define base00 #181818
#define base01 #282828
#define base02 #383838
#define base03 #585858
#define base04 #b8b8b8
#define base05 #d8d8d8
#define base06 #e8e8e8
#define base07 #f8f8f8
#define base08 #ab4642
#define base09 #dc9656
#define base0A #f7ca88
#define base0B #a1b56c
#define base0C #86c1b9
#define base0D #7cafc2
#define base0E #ba8baf
#define base0F #a16946

*foreground:   		base05
#ifdef background_opacity
*background:		[background_opacity]base00
*background-alt:	[background_opacity]base01
#else
*background:   		base00
*background-alt:	base01
#endif
*cursorColor:  		base05

*color0:       		base00
*color1:       		base08
*color2:       		base0B
*color3:       		base0A
*color4:       		base0D
*color5:       		base0E
*color6:       		base0C
*color7:       		base05

*color8:       		base03
*color9:       		base09
*color10:      		base01
*color11:      		base02
*color12:      		base04
*color13:      		base06
*color14:      		base0F
*color15:		base07


