Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: grayscale
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

rofi.color-enabled: true

!			bg       border   separator
rofi.color-window:	#181818, #263b4c, #181818

! State			bg       fg       bgalt    hlbg     hlfg	
rofi.color-normal:	#181818, #585858, #181818, #181818, #d8d8d8
rofi.color-active:	#181818, #d8d8d8, #181818, #181818, #d8d8d8
rofi.color-urgent:	#181818, #d8d8d8, #181818, #181818, #d8d8d8
rofi.font:		Roboto 15
rofi.bw: 		3
rofi.padding:		32
rofi.width:		50
rofi.lines:		10
rofi.modi:		window,run,ssh,drun

st.termname: st-256color
st.font: Iosevka Term:Semibold:pixelsize=14
st.borderpx: 16
st.mouseScrollLines: 5
st.bold_font: 1
st.actionfps: 60

URxvt*font: xft:Iosevka Term:Semibold:size=11,xft:Symbola
URxvt*scrollBar: false
URxvt*transparent: false
URxvt*internalBorder: 16

URxvt*keysym.Shift-Control-V: eval:paste_clipboard
URxvt*keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt*iso14755: False

URxvt*perl-ext-common: default,resize-font

URxvt*letterSpace: -1

xterm*utf8: 1

! special
*.foreground:   #dedede
*.background:   #121212
*.cursorColor:  #dedede

! black
*.color0:       #121212
*.color8:       #dedede

! red
*.color1:       #e27373
*.color9:       #ffa1a1

! green
*.color2:       #94b979
*.color10:      #bddeab

! yellow
*.color3:       #ffba7b
*.color11:      #ffdca0

! blue
*.color4:       #97bedc
*.color12:      #b1d8f6

! magenta
*.color5:       #e1c0fa
*.color13:      #fbdaff

! cyan
*.color6:       #00988e
*.color14:      #1ab2a8

! white
*.color7:       #dedede
*.color15:      #ffffff
